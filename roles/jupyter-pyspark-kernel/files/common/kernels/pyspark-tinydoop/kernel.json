{
  "argv": [
    "/usr/bin/tinydoop_start.sh",
    "/opt/jupyter-pyspark-kernel/bin/python",
    "-m",
    "ipykernel_launcher",
    "-f",
    "{connection_file}"
 ],
 "display_name": "Py3 PySpark - Tinydoop",
 "language": "python",
 "env": {
    "PYSPARK_DRIVER_PYTHON": "/opt/jupyter-pyspark-kernel/bin/python",
    "PYSPARK_PYTHON": "./environment/bin/python",
    "PYSPARK_SUBMIT_ARGS": "--master yarn-client --conf spark.yarn.appMasterEnv.PYSPARK_PYTHON=./environment/bin/python --archives /opt/jupyter-pyspark-kernel.tar.gz#environment --conf spark.dynamicAllocation.enabled=true --conf spark.dynamicAllocation.schedulerBacklogTimeout=30s --conf spark.dynamicAllocation.cachedExecutorIdleTimeout=3600s --conf spark.app.name=jupyter-notebook pyspark-shell"
  }
}
